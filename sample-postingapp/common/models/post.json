{
	"name": "post",
	"base": "PersistedModel",
	"idInjection": true,
	"options": {
		"validateUpsert": true
	},
	"properties": {
		"created": {
			"type": "date",
			"required": false
		},
		"text": {
			"type": "string",
			"required": true
		},
		"ownerId": {
			"type": "string",
			"required": true
		}
	},
	"validations": [],
	"relations": {
		"creator": {
			"type": "belongsTo",
			"model": "member",
			"foreignKey": "ownerId"
		}
	},
	"acls": [
	         {
	        	 "accessType": "*",
	        	 "principalType": "ROLE",
	        	 "principalId": "$everyone",
	        	 "permission": "DENY"
	         },{
	        	 "accessType": "*",
	        	 "principalType": "ROLE",
	        	 "principalId": "$unauthenticated",
	        	 "permission": "DENY"
	         },
	         {
	        	 "accessType": "EXECUTE",
	        	 "principalType": "ROLE",
	        	 "principalId": "$authenticated",
	        	 "permission": "ALLOW",
	        	 "property": "create"
	         },
	         {
	        	 "accessType": "EXECUTE",
	        	 "principalType": "ROLE",
	        	 "principalId": "$owner",
	        	 "permission": "ALLOW",
	        	 "property": ["updateAttributes","deleteById"]
	         },
	         {
	        	 "accessType": "READ",
	        	 "principalType": "ROLE",
	        	 "principalId": "$everyone",
	        	 "permission": "ALLOW"
	         }
	         ],
	         "methods": {}
}
